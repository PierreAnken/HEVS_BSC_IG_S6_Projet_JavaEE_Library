<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>UserSession</managed-bean-name>
		<managed-bean-class>library.managedbeans.UserSession</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>LoadMoney</managed-bean-name>
		<managed-bean-class>library.managedbeans.LoadMoney</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
	    	<property-name>userSession</property-name>
	    	<value>#{UserSession}</value>
	    </managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>Shelfs</managed-bean-name>
		<managed-bean-class>library.managedbeans.Shelfs</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
	    	<property-name>userSession</property-name>
	    	<value>#{UserSession}</value>
	    </managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>CreateAccount</managed-bean-name>
		<managed-bean-class>library.managedbeans.CreateAccount</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
	    	<property-name>userSession</property-name>
	    	<value>#{UserSession}</value>
	    </managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>Door</managed-bean-name>
		<managed-bean-class>library.managedbeans.Door</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
	    	<property-name>userSession</property-name>
	    	<value>#{UserSession}</value>
	    </managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>NavigationController</managed-bean-name>
		<managed-bean-class>library.managedbeans.NavigationController</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
	    	<property-name>userSession</property-name>
	    	<value>#{UserSession}</value>
	    </managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>RentBook</managed-bean-name>
		<managed-bean-class>library.managedbeans.RentBook</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
	    	<property-name>userSession</property-name>
	    	<value>#{UserSession}</value>
	    </managed-property>
	</managed-bean>
</faces-config>
